<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">



	<title>M Animerag</title>

	<link href="https://jerushas2597.github.io/myanimerag.github.io/atom.xml" rel="self"/>

	<link href="https://jerushas2597.github.io/myanimerag.github.io/ />

	<updated>{{ site.time | date_to_xmlschema }}</updated>

	<id>https://jerushas2597.github.io/myanimerag.github.io/</id>

	<author>

		<name>Tomoki Ken</name>

		<email>tomokitimeforeve@gmail.com</email>

	</author>



	{% for post in site.posts %}

		<entry>

			<title>{{ post.title }}</title>

			<link href="https://jerushas2597.github.io/myanimerag.github.io/{{ post.url }}"/>

			<updated>{{ post.date | date_to_xmlschema }}</updated>

			<img src="{{ post.image }}">

			<id>https://jerushas2597.github.io/myanimerag.github.io/{{ post.id }}</id>

			<content type="html">{{ post.content | xml_escape }}</content>

		</entry>

	{% endfor %}



</feed>
